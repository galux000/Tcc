@{
    ViewBag.Title = "Home Page";
}
@section featured {
<section class="featured">
    <!-- Main jumbotron for a primary marketing message or call to action -->
    <div class="jumbotron">
      
        <h1>Está procurando música independente?</h1>
        <p>Então está no lugar certo! *NomeAPP* é o lugar aonde você pode encontrar os melhores músicos independentes do país e ouvir suas músicas, isso tudo totalmente de graça!</p>
        <p><a class="btn btn-primary btn-lg" role="button">Learn more &raquo;</a></p>
      
    </div>
</section>
    
      
    
}
<div class="container">
      <!-- Example row of columns -->
      <div class="row">
        <div class="col-md-4">
          <h2>Para ouvintes</h2>
          <p>Aqui é o lugar aonde você pode encontrar a maior variedade de músicas independentes. Crie sua playlist, ouça de graça, compre música quando achar necessário! </p>
          <p><a class="btn btn-default" href="#" role="button">View details &raquo;</a></p>
        </div>
        <div class="col-md-4">
          <h2>Para Músicos</h2>
          <p>Está dificil a vida? Coloque sua música em nosso site, libere uma prévia, coloque para vender, lucre com seu trabalho.</p>
          <p><a class="btn btn-default" href="#" role="button">View details &raquo;</a></p>
       </div>

      </div>
</div>
